@using DotRezCore.Api.Tests.Integration.ModelGenerator.Extensions
@using DotRezCore.Api.Tests.Integration.ModelGenerator.Templating.Models
@inherits RazorLight.TemplatePage<DotRezEnumTemplateModel>
@this.FileHeader();
namespace @Model.Namespace
{
    @Model.TypeAccessModifier enum @Model.Name
    {
        @foreach (var enumeration in @Model.Enums)
        {
            if (@Model.IsStringEnum)
            {
        @:[System.Runtime.Serialization.EnumMember(Value = "@enumeration.Value")]
            }
        @:@enumeration.Name = @enumeration.InternalValue,
        }
    }
}